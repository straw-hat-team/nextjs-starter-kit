{
  "name": "@webapps/web1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next",
    "build": "paraglide-js compile --project ./project.inlang && next build",
    "start": "next start",
    "lint:eslint": "next lint",
    "format:prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern --write '**/*'",
    "test:vitest": "vitest --passWithNoTests --pool forks",
    "test:playwright:headless": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "postinstall": "paraglide-js compile --project ./project.inlang"
  },
  "browserslist": [
    "extends @straw-hat/browserslist-config"
  ],
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@inlang/paraglide-js-adapter-next": "1.1.0",
    "@monoturborepo/ui": "workspace:*",
    "@t3-oss/env-core": "0.9.2",
    "@t3-oss/env-nextjs": "0.9.2",
    "@vercel/otel": "1.8.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "next": "14.1.4",
    "next-auth": "5.0.0-beta.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "tailwindcss": "3.4.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.3.5",
    "@monoturborepo/tailwind-config": "workspace:*",
    "@next/bundle-analyzer": "14.1.4",
    "@playwright/test": "1.42.1",
    "@straw-hat/browserslist-config": "0.2.0",
    "@straw-hat/eslint-config-nextjs": "4.0.0",
    "@straw-hat/prettier-config": "3.1.5",
    "@straw-hat/tsconfig": "4.2.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/eslint": "8.56.6",
    "@types/node": "20.12.2",
    "@types/react": "18.2.73",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.4.0",
    "autoprefixer": "10.4.19",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "jsdom": "24.0.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vitest": "1.4.0"
  }
